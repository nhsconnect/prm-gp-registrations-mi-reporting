index="$index$"

{% from 'gp2gp_integration_8_days_snapshot_report/gp2gp_integration_8_days_snapshot_report_base.splunk' import
gp2gp_integration_8_days_snapshot_report_base %}

{{ gp2gp_integration_8_days_snapshot_report_base() }}

| eval integrated_under_8_days_percent = round((integrated_on_time/total_successful_integration) * 100, 2)
| eval integrated_after_8_days_percent = round((integrated_after_8_days/total_successful_integration) * 100, 2)

| eval successful_integration_status = mvappend("Integrated on time", "Integrated after 8 days")
| eval percent = mvappend(integrated_under_8_days_percent, integrated_after_8_days_percent)

| eval soft_zip = mvzip(successful_integration_status, percent)
| mvexpand soft_zip

| rex field=soft_zip "(?<successful_integration_status>.*),(?<percent>.*)"
| table successful_integration_status, percent
