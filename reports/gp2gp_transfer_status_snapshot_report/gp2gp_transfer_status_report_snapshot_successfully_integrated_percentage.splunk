index="$index$"

{% from 'gp2gp_transfer_status_snapshot_report/gp2gp_transfer_status_report_snapshot_base.splunk' import
gp2gp_transfer_status_report_snapshot_base %}


{{ gp2gp_transfer_status_report_snapshot_base() }}

| eval count_not_successfully_integrated = count_rejected + count_awaiting_integration + count_in_progress + count_technical_failure + count_unknown
| eval percentage_not_successfully_integrated =round((count_not_successfully_integrated/total_eligible_for_electronic_transfer) * 100,2)

| eval label=mvappend("Successfully integrated", "Not successfully integrated")
| eval count=mvappend(percentage_successfully_integrated, percentage_not_successfully_integrated)

| eval soft_zip = mvzip(label, count)
| mvexpand soft_zip

| rex field=soft_zip "(?<label>.*),(?<count>.*)"
| table label, count